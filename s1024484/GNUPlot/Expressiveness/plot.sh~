#! /bin/sh 



cat << __EOF | gnuplot

set term png
set output  "Expressiveness.png"
set title "Expressiveness Checks"

set xlabel 'lines of Akka Code (Log10)'
set ylabel 'lines of Code (Log10)'


set xrange [0:6]
set yrange [0:6]

plot "exp.dat" using 1:2 with points  title "Modified TAkka Lines" lc 1 ,\
     "exp.dat" using 1:3 with points  title "TAkka Code Lines" lc 2


set output

__EOF

